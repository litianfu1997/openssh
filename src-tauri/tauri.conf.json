{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "OpenSSH",
  "version": "0.2.4",
  "identifier": "com.openssh.client",
  "build": {
    "frontendDist": "../out/renderer",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run vite:dev",
    "beforeBuildCommand": "npm run vite:build"
  },
  "app": {
    "windows": [
      {
        "title": "OpenSSH",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}